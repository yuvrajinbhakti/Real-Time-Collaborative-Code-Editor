  "scripts": {
    "start:front": "react-scripts start",
    "start": "npm run build && node server-simple.js",
    "start:simple": "node server-simple.js",
    "start:ai": "node server.js",
    "start:enhanced": "node server-enhanced.js",
    "start:redis": "redis-server --port 6379",
    "start:dev": "concurrently \"npm run start:redis\" \"nodemon server.js\"",
    "build": "CI=false react-scripts build",
    "build:ci": "react-scripts build",
    "build:render": "SKIP_PREFLIGHT_CHECK=true CI=false GENERATE_SOURCEMAP=false react-scripts build",
    "build:no-check": "SKIP_PREFLIGHT_CHECK=true CI=false GENERATE_SOURCEMAP=false TSC_COMPILE_ON_ERROR=true DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "dev": "nodemon dev-server.js",
    "test": "react-scripts test",
    "test:load": "artillery run load-test.yml",
    "eject": "react-scripts eject",
    "railway:build": "npm install && npm run build",
    "railway:start": "node server-simple.js",
    "monitoring": "node monitoring/metrics-server.js",
    "docker:build": "docker build -t collab-editor .",
    "docker:run": "docker run -p 5000:5000 collab-editor",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose up --build -d",
    "docker:prod:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:app": "docker-compose logs -f app",
    "docker:clean": "docker system prune -af && docker volume prune -f",
    "setup": "./setup-enhanced.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.15",
    "vercel": "^30.2.3"
  },
  "overrides": {
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "schema-utils": "^4.2.0"
  },
  "resolutions": {
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "schema-utils": "^4.2.0",
    "react-scripts/fork-ts-checker-webpack-plugin": "^6.5.3"
  }
}
